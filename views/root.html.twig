<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <link href="css/bootstrap.min.css" rel="stylesheet" type="text/css">
        <link href="css/styles.css" rel="stylesheet" type="text/css">
        <script src="js/jquery-3.1.1.js"></script>
        <script src="js/scripts.js"></script>
        <title>Queen</title>
    </head>
    <body>
        <div class="container">
            {% for i in 1..8 %}
            <div id="row{{i}}"class="row">
                {% for j in 1..8%}
                <div id="col{{j}}" class="col-xs-1">
                </div>
                {% endfor %}
            </div>
            {% endfor %}

            <button id="move-queen-button">Move Queen</button>
            <button id="move-knight-button">Move Knight</button>
            <button id="move-rook-button">Move Rook</button>
            <button id="move-bishop-button">Move Bishop</button>
            <button id="move-king-button">Move King</button>
            <div class="result">
            </div>
        </div>
    </body>
</html>
